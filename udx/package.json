{
    "name": "@udayahire2/udx-ui",
    "version": "0.0.5",
    "description": "Reusable UI library for udx",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "udx": "scripts/cli.js"
    },
    "files": [
        "dist",
        "src",
        "scripts",
        "registry.json"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./styles.css": "./dist/styles.css"
    },
    "scripts": {
        "build": "tsup src/index.ts src/styles.css --format cjs,esm --dts --clean",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "keywords": [
        "ui",
        "react",
        "library"
    ],
    "author": "Udroid",
    "license": "ISC",
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "chalk": "^4.1.2",
        "commander": "^14.0.2",
        "fs-extra": "^11.3.3",
        "ora": "^5.4.1",
        "tsup": "^8.3.5",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@radix-ui/react-separator": "^1.1.8",
        "@react-three/drei": "^10.7.7",
        "@react-three/fiber": "^9.5.0",
        "@types/three": "^0.182.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "framer-motion": "^12.23.26",
        "gsap": "^3.14.2",
        "tailwind-merge": "^3.4.0",
        "three": "^0.182.0"
    }
}
